package com.ssn.fedral.gov;

import java.io.FileNotFoundException;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ApplicationContext;
import org.springframework.context.annotation.Import;

import com.ssn.fedral.gov.ssnservice.ISsnService;
import com.ssn.fedral.gov.stateConfig.AppConfig;

@SpringBootApplication
@Import(AppConfig.class)
public class SsnWaveApplication {

	public static void main(String[] args) throws FileNotFoundException {
		ApplicationContext ctx=null;
		ISsnService service=null;
		SpringApplication.run(SsnWaveApplication.class, args);
		service=ctx.getBean("stservice",ISsnService.class);
		System.out.println(service.registerEmployee());
	}

}
